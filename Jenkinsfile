pipeline {
  agent any
  
  stages {
    
    stage("build") {
      
      steps {
        echo 'Building the application'
        echo 'Application builted'
      }
    }
    
    stage("test") {
      
      steps {
        echo 'Testing the application'
        echo 'Application tested'
      }
    }
    
    stage("deploy") {
      
      steps {
        echo 'deploying the application'
      }
    }
  }
}
